<article class="module width_3_quarter">
<header>
  <h3 class="tabs_involved">Listagem de fotos</h3>
</header>

<div class="tab_container">
<div id="tab1" class="tab_content">
	<label>Titulo:<%= @gallery.title %></label><br>
	<label>Url: <%= @gallery.url %></label><br>
	<label><%= @gallery.body %></label><br>

<%= image_tag @gallery.foto.url(:thumb) %>
<%= image_tag @gallery.foto.url(:medium) %>
<%= image_tag @gallery.foto.url(:small) %>
<br>
<%= link_to admin_galleries_path, :class =>"botaocancelar" do %>
    Voltar <% end %>
 

<br>

</div><!-- end of #tab1 -->  

</div><!-- end of .tab_container -->

</article><!-- end of content manager article -->

<!-- render menudireito -->
<%= render  'admin/homes/menudireito' %>

<!-- fecha render menudireito -->

<div class="clear"></div> 


</article><!-- end of styles article -->
<div class="spacer"></div>