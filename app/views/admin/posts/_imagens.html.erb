<article class="module width_quarter">
<header><h3>Imagens do site</h3></header>
<div class="message_list">
<div class="module_content">                

<div id=".media">
<div class="media">
<%- @galleries.each do |g| -%>
<%= link_to image_tag(g.foto.url(:thumb)) , g.foto.url(:small), :target => "_blank"  %>
<%- end -%>
</div>
</div>


</div>
</div>
<footer>
<%= will_paginate @galleries %>
</footer>
</article><!-- end of messages article -->