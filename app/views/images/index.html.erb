<div class="12u">
<div class="5grid-layout">
<div class="row">
<% @galleries.each do |g| %>
    <div class="4u">
      <section>
     
       <a href="<%= image_path g.foto.url(:small) %>" rel="lightbox[roadtrip]" title="<%= g.title %> " > 
      <%= image_tag g.foto.url(:medium) %>
      </a>

      </section>
    </div>
<% end %>


</div>
<%= will_paginate @galleries %>
</div>
</div>



